{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "demo-app": {
      "projectType": "application",
      "root": "projects/demo-app",
      "sourceRoot": "projects/demo-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo-app",
            "index": "projects/demo-app/src/index.html",
            "main": "projects/demo-app/src/main.ts",
            "polyfills": [],
            "tsConfig": "projects/demo-app/tsconfig.app.json",
            "assets": [
              "projects/demo-app/src/favicon.ico",
              "projects/demo-app/src/assets"
            ],
            "styles": [
              "projects/demo-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true
            },
            "development": {
              "optimization": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-app:build:production"
            },
            "development": {
              "browserTarget": "demo-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/demo-app/src/test.ts",
            "karmaConfig": "projects/demo-app/karma.conf.js",
            "polyfills": [],
            "tsConfig": "projects/demo-app/tsconfig.spec.json",
            "scripts": [],
            "styles": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/demo-app/**/*.ts",
              "projects/demo-app/**/*.html"
            ],
            "eslintConfig": "projects/demo-app/eslint.config.js"
          }
        }
      }
    },
    "dual-listbox": {
      "projectType": "library",
      "root": "projects/dual-listbox",
      "sourceRoot": "projects/dual-listbox/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/dual-listbox/tsconfig.lib.json",
            "project": "projects/dual-listbox/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/dual-listbox/tsconfig.lib.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/dual-listbox/**/*.ts",
              "projects/dual-listbox/**/*.html"
            ],
            "eslintConfig": "projects/dual-listbox/eslint.config.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "warnings": {
      "versionMismatch": false
    }
  }
}